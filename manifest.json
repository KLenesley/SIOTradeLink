{
  "manifest_version": 3,
  "name": "SIOTradeLink",
  "version": "1.5",
  "description": "Redirection du jeu de bourse planspiel vers boursorama pour les SIO",
  "permissions": [],
  "host_permissions": [
    "https://trading.planspiel-boerse.de/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://trading.planspiel-boerse.de/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_icon": "SIOBourse.png",
    "default_title": "Redirection vers Boursorama"
  },
  "icons": {
    "16": "SIOBourse.png",
    "48": "SIOBourse.png",
    "128": "SIOBourse.png"
  }
}
